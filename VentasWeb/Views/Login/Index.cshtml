
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="~/Content/css/login.css" />
</head>
<body>
    <div class="wrapper">
        <form action="@Url.Content("~/Login/Index")" method="post">
            <img src="/imagenes/logo.png" width="230px" height="100px" />
            <div class="input-box">
                <input class="input-box form-control mb-3" type="email" name="correo" required="" placeholder="Correo" autocomplete="off" value="admin@gmail.com">
                <i class='bx bxs-user'></i>
            </div>
            <div class="input-box">
                <input class="input-box form-control mb-3" type="password" required="" name="clave" placeholder="Contraseña" autocomplete="off" value="admin@gmail.com">
                <i class='bx bxs-lock-alt'></i>
            </div>
            <div class="d-grid gap-2 mb-3">
                <button class="btn btn-primary" type="submit">Ingresar</button>
            </div>
        </form>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
